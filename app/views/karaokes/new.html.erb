<h2>Add New Song</h2>

<table>
<%= form_for @karaoke do |karaoke_form| %>	
	<tr>
		<td>Code</td>
		<td><%= karaoke_form.text_field :code %></td>
	</tr>
	<tr>
		<td>Name</td>
		<td><%= karaoke_form.text_field :name %></td>
	</tr>
	<tr>
		<td colspan="2">
			<%= karaoke_form.submit %>
		</td>
	</tr>
<% end %>
</table>